configurations[fixed][1-col][c] = default

layout[<global>] = body-layout-fixed-acb

palette[id] = "8A0AA135-0AEB-F0C9-F773BC173B064C48"
palette[name] = "Impact"
palette[author] = "Acquia"
palette[white][hex] = FFFFFF
palette[white][name] = "White"
palette[black][hex] = 121212
palette[black][name] = "Charcoal"
palette[a][hex] = 62a734
