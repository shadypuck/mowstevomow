selectorMap[.active .active] = .tabs .tabs .active a
selectorMap[.tabs li a] = .tabs .tabs li a
selectorMap[#navigation .block .menu .active-trail .active] = #navigation .block .menu .active-trail a
selectorMap[#sidebar-a .block .content] = #sidebar-a .block .content-with-title
selectorMap[#banner-region .active .rb-background-image] = '#banner-region .rb-slide'
selectorMap[#banner-region .block .rb-slides .rb-slide] = '#banner-region .rb-slide'

layout[forum] = 'body-layout-fixed-c'
layout[forum*] = 'body-layout-fixed-c'

settings[block_everything][site_name] = header
settings[block_everything][site_slogan] = precontent_bottom
settings[block_everything][logo] = header
settings[viewport][content] = 'width=device-width, initial-scale=1.0'
settings[viewport][enabled] = 0

